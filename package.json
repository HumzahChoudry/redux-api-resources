{
  "name": "redux-api-resources",
  "version": "0.8.9",
  "description": "Opinioned library for working with remote API resources and redux",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "directories": {
    "dist": "dist",
    "docs": "docs",
    "examples": "examples",
    "src": "src"
  },
  "scripts": {
    "build": "npm run build:dist && npm run build:es && npm run build:umd && npm run build:umd:min",
    "build:dist": "babel src --out-dir dist",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "build:umd": "cross-env NODE_ENV=development webpack src/index.js dist/redux-form.js",
    "build:umd:min": "cross-env NODE_ENV=production webpack src/index.js dist/redux-form.min.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watchAll --notify"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/okbreathe/redux-api-resources.git"
  },
  "keywords": [
    "redux",
    "api",
    "reducers",
    "actions"
  ],
  "author": "Asher Van Brunt",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/okbreathe/redux-api-resources/issues"
  },
  "homepage": "https://github.com/okbreathe/redux-api-resources#readme",
  "dependencies": {
    "pluralize": "^3.1.0",
    "query-string": "^4.2.0",
    "url-join": "^1.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.23.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.3.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.22.0",
    "cross-env": "^3.2.4",
    "fetch-mock": "^5.9.4",
    "jest": "^19.0.2",
    "redux": "^3.6.0",
    "redux-mock-store": "^1.2.2",
    "redux-thunk": "^2.2.0",
    "regenerator-runtime": "^0.10.3",
    "webpack": "^2.2.1"
  },
  "jest": {
    "transform": {
      "js$": "./node_modules/babel-jest"
    },
    "moduleFileExtensions": [
      "js"
    ]
  }
}
