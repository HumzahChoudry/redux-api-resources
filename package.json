{
  "name": "redux-api-resources",
  "version": "2.0.3",
  "description": "Opinioned library for working with collections of resources through redux",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "dist": "dist",
    "docs": "docs",
    "examples": "examples",
    "src": "src"
  },
  "scripts": {
    "prepare": "yarn run build",
    "build": "rm -rf ./dist && tsc",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watchAll --notify"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/okbreathe/redux-api-resources.git"
  },
  "keywords": [
    "redux",
    "api",
    "reducers",
    "actions"
  ],
  "author": "Asher Van Brunt",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/okbreathe/redux-api-resources/issues"
  },
  "homepage": "https://github.com/okbreathe/redux-api-resources#readme",
  "dependencies": {
    "@types/redux-mock-store": "^0.0.10",
    "dts-webpack-plugin": "^0.0.9",
    "redux": "^3.7.2",
    "ts-loader": "^2.3.2",
    "typescript": "^2.5.2"
  },
  "devDependencies": {
    "@types/jest": "^20.0.7",
    "dts-bundle": "^0.7.3",
    "jest": "^20.0.4",
    "redux-mock-store": "^1.2.3",
    "redux-thunk": "^2.2.0",
    "ts-jest": "^20.0.10",
    "webpack": "^3.5.5"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "fixtures"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  }
}
